{
  "name": "svite-template-routify-ts",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p dev:routify dev:vite",
    "dev:routify": "routify run",
    "dev:vite": "vite",
    "build": "run-s build:routify build:vite",
    "build:routify": "routify run -b",
    "build:vite": "vite build",
    "serve": "vite preview",
    "validate": "svelte-check",
    "comb": "npx csscomb comb.css --config comb.json"
  },
  "dependencies": {
    "@roxi/routify": "^2.18.0",
    "mdsvex": "^0.9.8",
    "rehype-autolink-headings": "^5.1.0",
    "rehype-slug": "^4.0.1",
    "remark-abbr": "^1.4.1",
    "remark-github": "^10.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-order": "^4.1.0",
    "vite-main-js": "^0.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.2",
    "@supabase/postgrest-js": "^0.28.4",
    "@supabase/supabase-js": "^1.18.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "@tsconfig/svelte": "^1.0.13",
    "@types/cssnano": "^4.0.0",
    "@types/cuid": "^1.3.1",
    "@types/prettier": "^2.3.1",
    "@types/pug": "^2.0.4",
    "autoprefixer": "^10.2.6",
    "cssnano": "^4.1.11",
    "cuid": "^2.1.8",
    "dexie": "^3.0.3",
    "dotenv": "^8.6.0",
    "export-from-json": "^1.4.0",
    "jsontokens": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "pug": "^3.0.2",
    "svelte": "^3.38.3",
    "svelte-check": "^1.6.0",
    "svelte-hmr": "^0.14.4",
    "svelte-preprocess": "^4.7.3",
    "tippy.js": "^6.3.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "validator": "^13.6.0",
    "vite": "^2.4.0"
  },
  "appConfig": {
    "port": 5000,
    "assetsDir": "static",
    "template": "dist/index.html",
    "distDir": "dist",
    "buildDir": "dist/assets",
    "script": "dist/main.js"
  },
  "routify": {
    "sourceDir": "static",
    "extensions": [
      "md",
      "svelte"
    ]
  }
}
